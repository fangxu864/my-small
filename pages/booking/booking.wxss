@charset "UTF-8";
/**index.wxss**/
page {
  background: #f5f5f5;
  font-size: 14px;
  color: #444444; }

.box.datepicker {
  height: 44px;
  line-height: 44px;
  flex-direction: row;
  display: flex;
  background: #fff;
  font-size: 14px;
  margin-top: 20rpx; }

.picker {
  height: 44px;
  width: 100%; }

.mainBody {
  margin-bottom: 60px; }

.ticketListUl {
  padding: 0;
  margin: 10px 0; }

.ticketListUl .item {
  padding: 20rpx;
  background: #fff;
  margin-top: 10px; }

.ticketListUl .item .tit {
  font-size: 16px;
  font-weight: bold;
  margin-bottom: 5px; }

.ticketListUl .item .price, .ticketListUl .item .store {
  font-size: 14px;
  color: #878787; }

.ticketListUl .item .price .yen, .ticketListUl .item .price .num {
  color: #f95137; }

.ticketListUl .item .price .num {
  font-size: 16px;
  font-weight: bold; }

.ticketListUl .item .storeNum {
  color: #777777;
  font-weight: bold; }

.ticketListUl .bcon {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center; }

.ticketListUl .countBox {
  display: flex;
  flex-direction: row;
