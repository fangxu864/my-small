.totalBar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 42px;
  background: #ffffff; }

.totalBar .totalBarCon {
  display: flex;
  height: 100%;
  align-items: center;
  justify-content: space-between; }

.totalBar .totalT {
  margin-left: 10px; }

.totalBar .totalBox {
  color: #f95137;
  font-size: 14px; }

.totalBar .totalBox .totalNum {
  font-size: 18px;
  font-weight: bold; }

.totalBar .submitBtn {
  width: 90px;
  height: 100%;
  line-height: 42px;
  text-align: center;
  background: #f95137;
  color: #ffffff; }

.totalBar .submitBtn.disable {
  background: #999999;
  color: #555555; }

.descFlag {
  font-size: 12px;
  padding: 2px 4px;
  color: #457ed8;
  border: 1px solid #457ed8;
  margin: 10rpx 10rpx 10rpx 0; }

.common-contact-box {
  z-index: 20;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  width: 100%;
  transition: height 0.5s; }

.common-contact-box .title {
  height: 40px;
  line-height: 40px;
  text-align: center;
  border-bottom: 1px solid #e5e5e5; }

.common-contact-box .bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 43px;
  line-height: 43px;
  text-align: center;
  border-top: 1px solid #e5e5e5;
  color: #457ed8; }

.common-contact-box .con .item {
  height: 45px;
  line-height: 45px;
  margin: 0 10px;
  overflow: hidden;
  border-bottom: 1px solid #e5e5e5; }

.common-contact-box .con .item .del-btn {
  color: #99997f;
  text-align: center;
  width: 20%; }

.common-contact-box .con .item .del-btn:active {
  color: #457ed8;
  background: #f2f2f2; }

.common-contact-box .con .name-tel {
  width: 80%;
  display: inline-block;
  padding-left: 10rpx;
  box-sizing: border-box; }

.common-contact-box .con .name-tel .name {
  display: inline-block;
  width: 160rpx;
  overflow: hidden;
  height: 45px;
  float: left;
  text-overflow: ellipsis; }

.common-contact-box .con .name-tel .tel {
  display: inline-block;
  padding-left: 20rpx; }

.common-contact-box .con .name-tel:active {
  background: #f2f2f2; }

.slideInUp {
  animation-duration: 0.2s;
  animation-fill-mode: both;
  animation-name: slideInUp; }

@keyframes slideInUp {
  from {
    transform: translate3d(0, 60%, 0); }
  to {
    transform: none; } }

.fadeIn {
  animation-duration: 0.2s;
  animation-fill-mode: both;
  animation-name: fadeIn; }

@keyframes fadeIn {
  from {
    opacity: 0; } }

/* 身份证列表 */
.idcard-list-wrap {
  z-index: 20;
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  width: 100%;
  transition: height 0.5s; }

.idcard-list-wrap .title {
  height: 40px;
  line-height: 40px;
  text-align: center;
  border-bottom: 1px solid #e5e5e5; }

.idcard-list-wrap .bottom {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 43px;
  line-height: 43px;
  border-top: 1px solid #e5e5e5;
  color: #457ed8;
  display: flex;
  justify-content: space-around;
  align-items: center; }

.idcard-list-wrap .bottom .btn {
