.ticketListUl .countBox .inp {
  width: 65px;
  text-align: center;
  color: #457ed8;
  line-height: 28px; }

.ticketListUl .countBox .btn {
  width: 38px;
  height: 28px;
  line-height: 28px;
  color: #457ed8;
  text-align: center; }

.ticketListUl .countBox .btn.disabled {
