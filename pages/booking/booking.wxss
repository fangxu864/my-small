.pft-dialog__cbtn {
  background-color: #ffffff;
  height: 43px;
  line-height: 43px;
  text-align: center;
  border-top: 1px solid #e5e5e5;
  color: #457ed8; }

.pft-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  background: rgba(0, 0, 0, 0.7);
  opacity: 0;
  transform: translateY(150%);
  transition: opacity 0.4s ease;
  /* display: none; */ }

.pft-dialog__con {
  position: fixed;
  bottom: 0;
  width: 750rpx;
  background: white;
  transform: translateY(150%);
  transition: all 0.4s ease;
  z-index: 11; }

.pft-dialog--show .pft-dialog__con {
  transform: translateY(0); }

.pft-dialog--show .pft-dialog__mask {
  opacity: 0.5;
  transform: translateY(0); }

.qurey-time-scenic {
